{
  "manifest_version": 3,
  "name": "ChronoSolver",
  "version": "3.0",
  "description": "Automatically extracts answers from API and completes quizzes",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "ChronoSolver",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://maya.technicalhub.io/*"],
      "js": ["interceptor.obf.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}
